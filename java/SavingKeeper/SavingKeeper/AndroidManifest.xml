<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="jie.java.android.savingkeeper"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="7" />

    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".SavingKeeper"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="SavingListActivity" android:label="@string/title_savinglistactivity"></activity>
        <activity android:name="BankListActivity" android:label="@string/title_banklist"></activity>
        <activity android:name="SavingDetailActivity" android:label="@string/menu_addsaving">
        </activity>
        <activity android:name="RateListActivity" android:label="@string/title_ratelist"></activity>
        <activity android:name="RateDetailActivity" android:label="@string/title_addrate"></activity>
    </application>
</manifest>